IF EXISTS(SELECT * FROM [dbo].[OfficerRoleMapping] WHERE [OfficerID] = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] = @User4))       		
DELETE FROM [OfficerRoleMapping] WHERE [OfficerID] = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] = @User4)
