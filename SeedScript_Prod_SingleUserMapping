---- Officer Details Updation
DECLARE @RandomDigit NVARCHAR(10) SET @RandomDigit =  LEFT(CAST(RAND()*1000000000 AS INT),5)
DECLARE @Code NVARCHAR(20) = @RandomDigit
DECLARE @GivenName NVARCHAR SET @GivenName = (SELECT GivenName FROM Users WHERE UserID = @AdminUser)
DECLARE @FamilyName NVARCHAR SET @FamilyName = (SELECT FamilyName FROM Users WHERE UserID = @AdminUser)
DECLARE @OficerIdNo VARCHAR(10) = @RandomDigit
DECLARE @StateId INT SET @StateId = 1

UPDATE [dbo].[Officer] SET [Code] = @Code +' '+'Officer1'+@GivenName+@FamilyName, [GivenName] = @GivenName+' '+'Officer1', [FamilyName] = @FamilyName+' '+'Officer1', [OfficerIDNo] = @OficerIdNo, 
[Rank] = 'Officer1', [StateID] = @StateId, [HHCPassword] = NULL, [MiddleNames] = NULL, [Initials] = NULL, [DigitalSignatureID] = '0' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] = @User4)


------------------------------------------------------------------------------------------------------------------------------------

SET @RandomDigit =  LEFT(CAST(RAND()*1000000000 AS INT),5)
SET @Code = @RandomDigit
SET @OficerIdNo = @RandomDigit
SET @StateId = 1

UPDATE [dbo].[Officer] SET [Code] = @Code+' '+ 'Officer2'+@GivenName+@FamilyName, [GivenName] = @GivenName+' '+'Officer2', [FamilyName] = @FamilyName+' '+'Officer2', [OfficerIDNo] = @OficerIdNo, 
[Rank] = 'Officer2', [StateID] = @StateId, [HHCPassword] = NULL, [MiddleNames] = NULL, [Initials] = NULL, [DigitalSignatureID] = '0' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] = @User5)


------------------------------------------------------------------------------------------------------------------------------------

SET @RandomDigit =  LEFT(CAST(RAND()*1000000000 AS INT),5)
SET @Code = @RandomDigit
SET @OficerIdNo = @RandomDigit
SET @StateId = 1

UPDATE [dbo].[Officer] SET [Code] = @Code+' '+ 'Officer3'+@GivenName+@FamilyName, [GivenName] = @GivenName+' '+'Officer3', [FamilyName] = @FamilyName+' '+'Officer3', [OfficerIDNo] = @OficerIdNo, 
[Rank] = 'Officer3', [StateID] = @StateId, [HHCPassword] = NULL, [MiddleNames] = NULL, [Initials] = NULL, [DigitalSignatureID] = '0' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] = @User6)


------------------------------------------------------------------------------------------------------------------------------------

SET @RandomDigit =  LEFT(CAST(RAND()*1000000000 AS INT),5)
SET @Code = @RandomDigit
SET @OficerIdNo = @RandomDigit
SET @StateId = 1

UPDATE [dbo].[Officer] SET [Code] = @Code+' '+ 'Officer4'+@GivenName+@FamilyName, [GivenName] = @GivenName+' '+'Officer4', [FamilyName] = @FamilyName+' '+'Officer4', [OfficerIDNo] = @OficerIdNo, 
[Rank] = 'Officer4', [StateID] = @StateId, [HHCPassword] = NULL, [MiddleNames] = NULL, [Initials] = NULL, [DigitalSignatureID] = '0' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] = @User7)


------------------------------------------------------------------------------------------------------------------------------------

SET @RandomDigit =  LEFT(CAST(RAND()*1000000000 AS INT),5)
SET @Code = @RandomDigit
SET @OficerIdNo = @RandomDigit
SET @StateId = 1

UPDATE [dbo].[Officer] SET [Code] = @Code+' '+ 'Officer5'+@GivenName+@FamilyName, [GivenName] = @GivenName+' '+'Officer5', [FamilyName] = @FamilyName+' '+'Officer5', [OfficerIDNo] = @OficerIdNo, 
[Rank] = 'Officer5', [StateID] = @StateId, [HHCPassword] = NULL, [MiddleNames] = NULL, [Initials] = NULL, [DigitalSignatureID] = '0' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] = @User8)


