-----Mention master user in below declaration-----
DECLARE @AdminUser NVARCHAR(20) SET @AdminUser ='EnterAdminUser'
-----Sub user will declare based on admin user-----
DECLARE @User1 NVARCHAR(20) SET @User1 = @AdminUser+'.001'
DECLARE @User2 NVARCHAR(20) SET @User2 = @AdminUser+'.002'
DECLARE @User3 NVARCHAR(20) SET @User3 = @AdminUser+'.003'
DECLARE @User4 NVARCHAR(20) SET @User4 = @AdminUser+'.004'
DECLARE @User5 NVARCHAR(20) SET @User5 = @AdminUser+'.005' 
DECLARE @User6 NVARCHAR(20) SET @User6 = @AdminUser+'.006'
DECLARE @User7 NVARCHAR(20) SET @User7 = @AdminUser+'.007'
DECLARE @User8 NVARCHAR(20) SET @User8 = @AdminUser+'.008'
DECLARE @User9 NVARCHAR(20) SET @User9 = @AdminUser+'.009'
DECLARE @User10 NVARCHAR(20) SET @User10 = @AdminUser+'.010'
DECLARE @User11 NVARCHAR(20) SET @User11 = @AdminUser+'.011'
	

----------------- Reset officers associated to users 
IF EXISTS(SELECT * FROM [dbo].[Officer] WHERE [AssociatedUserID] LIKE @AdminUser+'%')                     
UPDATE [dbo].[Officer] SET [AssociatedUserID] = Null, [OfficerNameForCorrespondence] = Null, [SupervisorUserID] = Null WHERE [AssociatedUserID] LIKE @AdminUser+'%';	    


----------------- Reset the UserGroupMappings associated to officers
IF EXISTS(SELECT * FROM [dbo].[UserGroupMappings] WHERE [UserID] LIKE @AdminUser+'%')                     
DELETE [dbo].[UserGroupMappings] WHERE [UserID] LIKE @AdminUser+'%'


---------------------------------------------------------------- User association to [Officer]----------------------------------------------------------------------------------
UPDATE [dbo].[Officer] SET [AssociatedUserID] = @User4, [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence1' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] IS NULL AND [GivenName] IS NOT NULL AND Status='0' AND [FamilyName] IS NOT NULL AND [StationCodeID] IS NOT NULL)
UPDATE [dbo].[Officer] SET [AssociatedUserID] = @User5, [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence2' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] IS NULL AND [GivenName] IS NOT NULL AND Status='0' AND [FamilyName] IS NOT NULL AND [StationCodeID] IS NOT NULL)
UPDATE [dbo].[Officer] SET [AssociatedUserID] = @User6, [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence3' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] IS NULL AND [GivenName] IS NOT NULL AND Status='0' AND [FamilyName] IS NOT NULL AND [StationCodeID] IS NOT NULL)
UPDATE [dbo].[Officer] SET [AssociatedUserID] = @User7, [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence4' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] IS NULL AND [GivenName] IS NOT NULL AND Status='0' AND [FamilyName] IS NOT NULL AND [StationCodeID] IS NOT NULL)
UPDATE [dbo].[Officer] SET [AssociatedUserID] = @User8, [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence5' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] IS NULL AND [GivenName] IS NOT NULL AND Status='0' AND [FamilyName] IS NOT NULL AND [StationCodeID] IS NOT NULL)
UPDATE [dbo].[Officer] SET [AssociatedUserID] = @User9, [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence6' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] IS NULL AND [GivenName] IS NOT NULL AND Status='0' AND [FamilyName] IS NOT NULL AND [StationCodeID] IS NOT NULL)
UPDATE [dbo].[Officer] SET [AssociatedUserID] = @User10, [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence7' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] IS NULL AND [GivenName] IS NOT NULL AND Status='0' AND [FamilyName] IS NOT NULL AND [StationCodeID] IS NOT NULL)
UPDATE [dbo].[Officer] SET [AssociatedUserID] = @User11, [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence8' WHERE ID = (SELECT TOP 1 ID FROM [dbo].[Officer] WHERE [AssociatedUserID] IS NULL AND [GivenName] IS NOT NULL AND Status='0' AND [FamilyName] IS NOT NULL AND [StationCodeID] IS NOT NULL)


------------------------------------------------- [SupervisorUserID] association to Users--------------------------------------------------------------------------------------------
UPDATE [dbo].[Officer] SET [SupervisorUserID] =(SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence1' AND [AssociatedUserID] = @User4) WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence1' AND [AssociatedUserID] = @User5
UPDATE [dbo].[Officer] SET [SupervisorUserID] =(SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence2' AND [AssociatedUserID] = @User5) WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence2' AND [AssociatedUserID] = @User6
UPDATE [dbo].[Officer] SET [SupervisorUserID] =(SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence3' AND [AssociatedUserID] = @User6) WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence3' AND [AssociatedUserID] = @User7
UPDATE [dbo].[Officer] SET [SupervisorUserID] =(SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence4' AND [AssociatedUserID] = @User7) WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence4' AND [AssociatedUserID] = @User8
UPDATE [dbo].[Officer] SET [SupervisorUserID] =(SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence4' AND [AssociatedUserID] = @User7) WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence5' AND [AssociatedUserID] = @User9
UPDATE [dbo].[Officer] SET [SupervisorUserID] =(SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence4' AND [AssociatedUserID] = @User7) WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence6' AND [AssociatedUserID] = @User10
UPDATE [dbo].[Officer] SET [SupervisorUserID] =(SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence4' AND [AssociatedUserID] = @User7) WHERE [OfficerNameForCorrespondence] = 'OfficerNameForCorrespondence7' AND [AssociatedUserID] = @User11


----------------------------------------[UserGroupMappings] for 8 group and 8 role--------------------------------------------------------------------------------------------
INSERT INTO [dbo].[UserGroupMappings] (UserID, GroupID) 
VALUES((SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence]='OfficerNameForCorrespondence1' AND [AssociatedUserID] = @User4),(SELECT ID FROM [UserGroups] WHERE [Code] = 'Code1'))
INSERT INTO [dbo].[UserGroupMappings] (UserID, GroupID) 
VALUES((SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence]='OfficerNameForCorrespondence2' AND [AssociatedUserID] = @User5),(SELECT ID FROM [UserGroups] WHERE [Code] = 'Code2'))
INSERT INTO [dbo].[UserGroupMappings] (UserID, GroupID) 
VALUES((SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence]='OfficerNameForCorrespondence3' AND [AssociatedUserID] = @User6),(SELECT ID FROM [UserGroups] WHERE [Code] = 'Code3'))
INSERT INTO [dbo].[UserGroupMappings] (UserID, GroupID) 
VALUES((SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence]='OfficerNameForCorrespondence1' AND [AssociatedUserID] = @User7),(SELECT ID FROM [UserGroups] WHERE [Code] = 'Code4'))
INSERT INTO [dbo].[UserGroupMappings] (UserID, GroupID) 
VALUES((SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence]='OfficerNameForCorrespondence5' AND [AssociatedUserID] = @User8),(SELECT ID FROM [UserGroups] WHERE [Code] = 'Code5'))
INSERT INTO [dbo].[UserGroupMappings] (UserID, GroupID) 
VALUES((SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence]='OfficerNameForCorrespondence6' AND [AssociatedUserID] = @User9),(SELECT ID FROM [UserGroups] WHERE [Code] = 'Code6'))
INSERT INTO [dbo].[UserGroupMappings] (UserID, GroupID) 
VALUES((SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence]='OfficerNameForCorrespondence7' AND [AssociatedUserID] = @User10),(SELECT ID FROM [UserGroups] WHERE [Code] = 'Code7'))
INSERT INTO [dbo].[UserGroupMappings] (UserID, GroupID) 
VALUES((SELECT TOP 1 [AssociatedUserID] FROM [dbo].[Officer] WHERE [OfficerNameForCorrespondence] ='OfficerNameForCorrespondence8' AND [AssociatedUserID] = @User11),(SELECT ID FROM [UserGroups] WHERE [Code] = 'Code8'))

----- UserGroupMappings for remaining 4 groups
INSERT Into [dbo].[UserGroupMappings] (UserID, GroupID) values(@AdminUser,(SELECT ID from [UserGroups] WHERE [Code] = 'Code1'))
INSERT Into [dbo].[UserGroupMappings] (UserID, GroupID) values(@User1,(SELECT ID from [UserGroups] WHERE [Code] = 'Code2'))
INSERT Into [dbo].[UserGroupMappings] (UserID, GroupID) values(@User2,(SELECT ID from [UserGroups] WHERE [Code] = 'Code3'))
