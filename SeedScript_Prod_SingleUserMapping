--CRYSTAL REPORT PERMISSIONS FOR USER SOVUser-----
DECLARE @reportID INT = (SELECT TOP 1 [ReportID] FROM [dbo].[CrystalReport] WHERE Code IN('EnterReportCode'))
DECLARE @userID VARCHAR(20)=@User4
IF NOT EXISTS(SELECT * FROM [CrystalReportPermission] WHERE [UserID] =  @userID AND [ReportID] = @reportID)
BEGIN
    INSERT INTO  [dbo].[CrystalReportPermission] ([UserID], [ReportID], [Permissions], [GroupID])
    VALUES( @userID,@reportID,'EnterPermissionId',Null)
END

Set @reportID = (SELECT TOP 1 [ReportID] FROM [dbo].[CrystalReport] WHERE Code IN('EnterReportCode'))
IF NOT EXISTS(SELECT * FROM [CrystalReportPermission] WHERE [UserID] = @userID AND [ReportID] = @reportID)
BEGIN
    INSERT INTO  [dbo].[CrystalReportPermission] ([UserID], [ReportID], [Permissions], [GroupID])
    VALUES( @userID,@reportID,'EnterPermissionId',Null)
END

Set @reportID = (SELECT TOP 1 [ReportID] FROM [dbo].[CrystalReport] WHERE Code IN('EnterReportCode'))
IF NOT EXISTS(SELECT * FROM [CrystalReportPermission] WHERE [UserID] =  @userID AND [ReportID] = @reportID)
BEGIN
    INSERT INTO  [dbo].[CrystalReportPermission] ([UserID], [ReportID], [Permissions], [GroupID])
    VALUES( @userID,@reportID,'EnterPermissionId',Null)
END

Set @reportID = (SELECT TOP 1 [ReportID] FROM [dbo].[CrystalReport] WHERE Code IN('EnterReportCode'))
IF NOT EXISTS(SELECT * FROM [CrystalReportPermission] WHERE [UserID] = @userID AND [ReportID] = @reportID)
BEGIN
    INSERT INTO  [dbo].[CrystalReportPermission] ([UserID], [ReportID], [Permissions], [GroupID])
    VALUES( @userID,@reportID,'EnterPermissionId',Null)
END

Set @reportID = (SELECT TOP 1 [ReportID] FROM [dbo].[CrystalReport] WHERE Code IN('EnterReportCode'))
IF NOT EXISTS(SELECT * FROM [CrystalReportPermission] WHERE [UserID] = @userID AND [ReportID] = @reportID)
BEGIN
    INSERT INTO  [dbo].[CrystalReportPermission] ([UserID], [ReportID], [Permissions], [GroupID])
    VALUES(@userID,@reportID,'EnterPermissionId',Null)
END

Set @reportID = (SELECT TOP 1 [ReportID] FROM [dbo].[CrystalReport] WHERE Code IN('EnterReportCode'))
IF NOT EXISTS(SELECT * FROM [CrystalReportPermission] WHERE [UserID] = @userID AND [ReportID] = @reportID)
BEGIN
    INSERT INTO  [dbo].[CrystalReportPermission] ([UserID], [ReportID], [Permissions], [GroupID])
    VALUES(@userID,@reportID,'EnterPermissionId',Null)
END

Set @reportID = (SELECT TOP 1 [ReportID] FROM [dbo].[CrystalReport] WHERE Code IN('EnterReportCode'))
